{
  "extends": ["next/babel"],
  "parser": "babel-eslint",
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  },
  "plugins": [
    "transform-runtime",
    "proposal-optional-chaining",
    "proposal-throw-expressions",
    "@babel/plugin-proposal-do-expressions"
  ]
}
